{
  "name": "smaug",
  "version": "0.1.0",
  "description": "Archive Twitter/X bookmarks to markdown with link expansion, content extraction, and AI-powered categorization",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "smaug": "src/cli.js"
  },
  "scripts": {
    "fetch": "node src/cli.js fetch",
    "process": "node src/cli.js process",
    "start": "node src/cli.js",
    "ui": "node src/cli.js ui",
    "ui:dev": "cd ui && npm run dev",
    "ui:build": "cd ui && npm run build",
    "download-videos": "node src/download-videos.js",
    "test": "node --test"
  },
  "keywords": [
    "twitter",
    "x",
    "bookmarks",
    "archive",
    "markdown",
    "claude-code"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "express": "^4.21.2"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=20"
  }
}
